nt('a');\n                a.href = url; url = a.href;\n                return url;\n            }\n            function getReletiveUrl(url) {\n                var reg = /^https?:\\/\\/[^\\/]+/;\n                return url.replace(reg, "");\n            }\n            function getFilename(href) {\n                var url = getReletiveUrl(getAbsoluteUrl(href));\n                if (url == "" || url == "/") return "index.html";\n                return url.substring(1).replace(/\\//g, "-").replace(/\\?/g, "_").replace(/=/g, "-").replace(/&/g, "_") + ".html";\n            }\n        });\n        rslt.push(pageobj);\n        var filepath = "html/"+ folder + '/' + pageobj.filename;\n        fs.write(filepath, pageobj.content, 'w');\n        visited.push(address);\n        console.log(address +" -> " + filepath );\n        for (var i = 0; i < pageobj.hrefs.length; i++) {\n            var href = pageobj.hrefs[i];\n            if (!isVisited(href)) doTask(href);\n        }\n    });\n}\n\nfunction wait() {\n    setTimeout(function () {\n        if (queue.length == visited.length) {\n            console.log( rslt.length + "/" + queue.length );\n            phantom.exit();\n        }\n        else {\n            wait();\n        }\n    }, 1000);\n}\nwait();\n	20	t	998,999	args=[url,folder]，爬取URL网页，生成到目标路径	2015-07-14 15:21:03	2017-04-27 10:48:03.790429
