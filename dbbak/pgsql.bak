dd_mvc	DD-MVC	class dd_mvc:\n    def _use(self, oschm, oname, omethod):\n        def ks(pyid):\n            return "use." + ".".join(pyid)\n        def ds(pyid):\n            oschm, oname, omethod = pyid\n            pyobj = dh.Obj("select opy pycmd from dd.dd_api where oschm=@oschm and oname=@oname and omethod=@omethod", oschm, oname, omethod)\n            if pyobj: return dc.Python.Exec(dc.Python.Compile(pyobj.pycmd))\n        def use(pyid):\n            if not pyid: return None\n            keys = pyid.split(".")\n            schm, name, method = [oschm, oname, omethod][0:3-len(keys)] + keys\n            return dc.Data.Tables(["dd.dd_api"]).Find([schm, name, method], ks, ds)\n        return use\n    def _content(self, html):\n        return controller.Action( {"content": html,  "contentType":"text/html"} )\n    def _error(self, error):\n        return controller.Action( { "transfer": "{0}?tips={1}".format(dc.Setting.ErrorPage, Server.UrlEncode(error)) } ) if Request.HttpMethod == "GET" else jh.Error(error)\n    def _ar(self, **keyargs):\n        return controller.Action(keyargs)\n    def _vs(self, oschm=None, oname=None, omethod=None):\n        vs = {}\n        vs["controller"], vs["Server"], vs["Request"], vs["Response"], vs["args"], vs["data"], vs["Session"], vs["use"], vs["ar"] = controller, Server, Request, Response, args, data, Session, self._use(oschm, oname, omethod), self._ar\n        return vs\n    def _viewkey(self, keyData):\n        return "view."+".".join(keyData)\n    def _viewdata(self, keyData):\n        oschm, oname, omethod = keyData\n        page = dh.Obj("select * from dd.dd_view where oschm=@oschm and oname=@oname and omethod=@omethod", oschm, oname, omethod)\n        if not page: return None\n        page.pycode = dc.Python.Compile(page.opy)\n        page.layout = page.olayout.split(".") if page.olayout else None\n        if page.layout:\n            if len(page.layout)==1: page.layout= ( page.oschm, page.oname, page.layout[0] )\n            if len(page.layout)==2: page.layout= ( page.oschm, page.layout[0], page.layout[1] )   \n        page.roles = page.oroles.split(",") if page.oroles else []\n        if page.odef: ph.dd.extend(page, jh.Decode(page.odef))\n        return page\n    def _view(self, key):\n        return dc.Data.Tables(["dd.dd_view"]).Find( key, self._viewkey, self._viewdata)\n    def _layout(self, page):\n        return self._view(page.layout) if page.layout else None\n    def _views(self, page):\n        pages = [ page ]\n        layout = self._layout(page)\n        while layout and len(pages)<10:\n            pages.append(layout)\n            layout = self._layout(layout)\n        return pages\n    def view(self):\n        if not args._oschm or not args._oname or not args._omethod: return self._error(dc.I18N.T("dd.NotFound"))\n        key = (args._oschm, args._oname, args._omethod)\n        page = self._view(key)\n        if not page: return self._error(dc.I18N.T("dd.NotFound"))\n        if page.oneedlogin:\n            if not args.uid: return self._error(dc.I18N.T("dd.Unauthorized"))\n            if page.oroles and (not args.urole or not linq(page.roles).Intersect(linq(args.urole.split(','))).ToArray()) : return self._error(dc.I18N.T("dd.Unauthorized"))\n        pages = self._views(page)\n        vs = self._vs(args._oschm, args._oname, args._omethod)\n        models = [ dc.Python.Exec(p.pycode,vs) if p.opy else ph.dd.dynamic() for p in pages]\n        models = [ ph.dd.dynamic(m) for m in models if m]\n        model = linq(models).Aggregate(lambda crt,nxt: ph.dd.extend(nxt,crt)) if models else ph.dd.dynamic()\n        if model.redirect: return controller.Action({ "redirect": model.redirect })\n        if model.transfer: return controller.Action({ "transfer": model.transfer })\n        if model.error: return self._error(model.error)\n        if args._model:\n            if args._model == "1": return model\n            if args._model == "2": return models[0]\n            if args._model and model[args._model] and callable(model[args._model]): return model[args._model]()\n            return model\n        html = linq([p.ohtml for p in pages]).Aggregate(lambda crt,nxt: nxt.replace('@RenderBody()', crt)).replace('@RenderBody()','')\n        return controller.Action( { "content": dc.Razor.Exec(html, model), "contentType":"text/html" })\n    def _api_key(self, keyData):\n        return "api." + ".".join(keyData)\n    def _api_data(self, keyData):\n        schm, name, method = keyData\n        python = dh.Obj("select * from dd.dd_api where oschm=@schm and oname=@name and omethod=@method", schm, name, method)\n        if not python: return None\n        python.code = dc.Python.Compile(python.opy)\n        python.roles = python.oroles.split(",") if python.oroles else []\n        if python.odef: ph.dd.extend(python, jh.Decode(python.odef))\n        return python\n    def api(self):\n        if not args._oschm or not args._oname or not args._omethod: return self._error(dc.I18N.T("dd.NotFound"))\n        python = dc.Data.Tables(["dd.dd_api"]).Find( (args._oschm, args._oname, args._omethod), self._api_key, self._api_data )\n        if not python: return jh.Error(dc.I18N.T("dd.NotFound"))\n        if python.oneedlogin:\n            if not args.uid: return jh.Error(dc.I18N.T("dd.Unauthorized"))\n            if python.oroles and (not args.urole or not linq(python.roles).Intersect(linq(args.urole.split(','))).ToArray()) : return jh.Error(dc.I18N.T("dd.Unauthorized"))\n        return dc.Python.Exec(python.code, self._vs(args._oschm, args._oname, args._omethod))\n    def _act_key(self, keyData):\n        return "act." + ".".join(keyData)\n    def _act_data(self, keyData):\n        schm, name, method = keyData\n        api = self._api_data(keyData)\n        view = self._viewdata(keyData)\n        if api and view: return self._error("Matched to multiple targets!")\n        if api or view: return (bool(api), api or view)\n    def act(self):\n        if not args._oschm 