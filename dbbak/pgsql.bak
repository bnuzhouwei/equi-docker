--
-- Name: dd_v_attdef; Type: VIEW; Schema: dd; Owner: postgres
--

CREATE VIEW dd_v_attdef AS
 SELECT concat(t7.nspname, '.', t2.relname, '.', t1.attname) AS attfullname,
    concat(t7.nspname, '.', t2.relname) AS relfullname,
    t7.nspname AS relschm,
    t2.relkind,
    t2.relname,
    t1.attnum,
    t1.attname,
    t3.typname AS atttype,
        CASE
            WHEN (t1.attlen > 0) THEN NULL::integer
            WHEN (t1.atttypmod = (-1)) THEN NULL::integer
            ELSE (t1.atttypmod - 4)
        END AS attlen,
    t1.attnotnull,
    t8.adsrc AS attdefault,
    COALESCE((t1.attnum = ANY ((t4.indkey)::smallint[])), false) AS attisprimary,
        CASE
            WHEN ("left"(t8.adsrc, 8) = 'nextval('::text) THEN true
            ELSE false
        END AS attisidentity,
    t6.description AS reldesc,
    t5.description AS attdesc
   FROM (((((((pg_attribute t1
     JOIN pg_class t2 ON ((t1.attrelid = t2.oid)))
     JOIN pg_type t3 ON ((t1.atttypid = t3.oid)))
     LEFT JOIN pg_index t4 ON (((t1.attrelid = t4.indrelid) AND (t4.indisprimary = true))))
     LEFT JOIN pg_description t5 ON (((t1.attrelid = t5.objoid) AND (t1.attnum = t5.objsubid))))
     LEFT JOIN pg_description t6 ON (((t1.attrelid = t6.objoid) AND (t6.objsubid = 0))))
     LEFT JOIN pg_namespace t7 ON ((t2.relnamespace = t7.oid)))
     LEFT JOIN pg_attrdef t8 ON (((t1.attrelid = t8.adrelid) AND (t1.attnum = t8.adnum))))
  WHERE (((t7.nspname <> ALL (ARRAY['information_schema'::name, 'pg_catalog'::name])) AND (t1.attnum > 0)) AND (t2.relkind = ANY (ARRAY['r'::"char", 'v'::"char"])));


ALTER TABLE dd_v_attdef OWNER TO postgres;

--
-- Name: dd_v_reldef; Type: VIEW; Schema: dd; Owner: postgres
--

CREATE VIEW dd_v_reldef AS
 SELECT DISTINCT dd_v_attdef.relfullname,
    dd_v_attdef.relkind,
    dd_v_attdef.relschm,
    dd_v_attdef.relname,
    dd_v_attdef.reldesc
   FROM dd_v_attdef;


ALTER TABLE dd_v_reldef OWNER TO postgres;

