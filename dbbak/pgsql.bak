={0}'.format(rel))\n            if href.find('/core/uform/query/') >=0 :\n                return jh.Href('/core/uform/modify/dd_libquery?ff=queryname&fv={0}'.format(rel))\n            if href.find('/core/uform/datagrid') >=0 or href.find('/core/uform/index') >=0 or href.find('/core/uform/modify') >=0:\n                return jh.Href('/core/uform/modify/dd_form?ff=formname&fv={0}'.format(rel))\n            if href.find('/core/ucontent/page/') >=0 :\n                return jh.Href('/core/uform/modify/dd_page?ff=oname&fv={0}'.format(rel))\n        matches = ph.dd.matches(href, '^/(api|view|web)/(.+?)/(.+?)/(.+)\\??')\n        if matches:\n            groups = matches[0].Groups\n            otype, schm, name, method = groups[1].Value, groups[2].Value, groups[3].Value, groups[4].Value\n            api = dh.Obj("select * from dd.dd_api where oschm=@oschm and oname=@oname and omethod=@omethod", schm, name, method)\n            view = dh.Obj("select * from dd.dd_view where oschm=@oschm and oname=@oname and omethod=@omethod", schm, name, method)\n            if api: return jh.Href("/core/uform/modify/dd_api?ff=id&fv={0}".format(api.id))\n            if view: return jh.Href("/core/uform/modify/dd_view?ff=id&fv={0}".format(view.id))\n        return jh.Error('Href {0} Not supported!'.format(href))\n    \n    def article_url(self):\n        "60 Redirect to the url of article which depends on id and alias."\n        if args.id or args.alias:\n            url = "/core/content/{1}/{0}".format(args.alias,args.type) if args.alias else "/core/content/{1}?id={0}".format(args.id,args.type)\n            return controller.Action({"redirect":url})\n        return "Please specify id or alias of the article."\n    \n    def element_default(self):\n        "70 Reset system fields. "\n        cmds = [\n            "update dd.dd_element set eenabled = 0, isdetail = 0 where attname in ('ddztm','ddztbz','ddztsj','insertedtime','updatedtime')",\n            "update dd.dd_element set isgrid = 0 where attname in ('ddztbz','insertedtime')",\n            "update dd.dd_element set isgrid = 1 where attname in ('ddztm','ddztsj','updatedtime')",\n            "update dd.dd_element set attdefault = '(getdate())' where attname in ('insertedtime','updatedtime')"\n        ]\n        if data:\n            cmds = [cmd + " and formid=@id" for cmd in cmds]\n        cmd = ";".join(cmds)\n        dbcommands = [dh.CreateCommand(cmd,row.id) for row in data] if data else dh.CreateCommand(cmd)\n        cnt = dh.ExecuteNonQuery(dbcommands)\n        return jh.Success(str(cnt))\n    \n    def element_vtype_maxlength(self):\n        "71 Add element maxLength validation."\n        cmds = []\n        for row in data:\n            elements = dh.ExecuteDynamicArray("select * from dd.dd_element where formid=@id", row)\n            for element in elements:\n                if element.attlen:\n                    vtype = element.vtype or ''\n                    vtypes = [v for v in vtype.split(";") if v and v.lower().find("maxlength") == -1 ]                    \n                    vtypes = vtypes + [ "maxLength:{0}".format(element.attlen) ]\n                    element.vtype = ";".join(vtypes)\n            cmds = cmds + [ dh.CreateCommand("update dd.dd_element set vtype=@vtype where id=@id", element) for element in elements ]\n        cnt = dh.ExecuteNonQuery(cmds)\n        dc.Form.Refresh()\n        return jh.Success(str(cnt))\n    \n    def href_def_to_file(self):\n        "80 Save the definition of button href to ~/app_data/temp/"\n        folds = ["python","sql","report","chart","query","page"]\n        folds = [Server.MapPath("~/temp/{0}".format(fold)) for fold in folds]\n        for fold in folds:\n            if not System.IO.Directory.Exists(fold):\n                System.IO.Directory.CreateDirectory(fold)                \n        pys = [(row.id, row.ocmd, "py", "python") for row in dh.ExecuteDynamicArray("select * from dd.dd_libpython where left(id,2)!='dd'")]\n        sqls = [(row.id, row.ocmd, "sql", "sql") for row in dh.ExecuteDynamicArray("select * from dd.dd_libsql where left(id,2)!='dd'")]\n        query = [(row.queryname, row.ocmd, "sql", "query") for row in dh.ExecuteDynamicArray("select * from dd.dd_libquery where left(queryname,2)!='dd'")]\n        report = [(row.oname, row.oview, "html", "report") for row in dh.ExecuteDynamicArray("select * from dd.dd_report where left(oname,2)!='dd'")]\n        page = [(row.oname, row.oview, "html", "page") for row in dh.ExecuteDynamicArray("select * from dd.dd_page where left(oname,2)!='dd'")]\n        chart = [(row.oname, row.osqlcmd, "html", "chart") for row in dh.ExecuteDynamicArray("select * from dd.dd_chart where left(oname,2)!='dd'")]\n        for obj in [pys, sqls, query, report, page, chart]:\n            self._save_to_file(obj)\n    def _save_to_file(self, id_cmd_ext_folders):\n        for (id,cmd,ext,folder) in id_cmd_ext_folders:\n            filename = Server.MapPath( "/app_data/temp/{0}/{1}.{2}".format(folder,id,ext))            \n            sw = System.IO.StreamWriter(filename)\n            sw.Write(cmd or "")\n            sw.Close()\n    \n    def dd_log_clear(self):\n        "90 Clear all the log."\n        cmd = "TRUNCATE table dd.dd_log;"\n        cnt = dh.ExecuteNonQuery(cmd)\n        return jh.Success("{0} logs cleared.".format(cnt))\n    \n    def dd_log_clear_same(self):\n        "91 Delete the logs with the same url and level."\n        logs = [ db.Set("dd_log").Find(row.id) for row in data ]\n        cmdText = "delete from dd.dd_log where loglevel=@loglevel and url=@url"\n        cmds = [dh.CreateCommand(cmdText,row) for row in logs]\n        cnt = dh.ExecuteNonQuery(cmds)\n        return jh.Success("{0} logs cleared.".format(cnt))\n    \n    def dd_file_manager(self):\n        "92 File manager for database, app_data, content, temp."\n        return controller.Action({ "transfer" : "/core/api/tab?data=[{url:'/core/uform/datagrid/dd_libfile',title:'database',iconCls:'fa-database'},{url:'/core/cms/filemanager/content',title:'/content',iconCls:'fa-eye'},{url:'/core/cms/filemanager/temp',title:'/temp',iconCls:'fa-trash-o'},{url:'/core/cms/filemanager/data',title:'/app_data',iconCls:'fa-eye-slash'}]" })\n    \n    def dd_element_eseq(self):\n        "93 Sync sequence of element in dd_element and database."\n        n = dh.Exec("update dd.dd_element set eseq = attnum * 10, enumber = attnum where formid = @formid;", args.formid)\n        return jh.Affect(n)\n    \n    def dd_task(self):\n        "100 返回任务模型"\n        return dc.Task.Exec(args)\n    \n    def dd_log_bak(self):\n        from System.Data import DataHelper\n        from System.IO import FileInfo\n        path = Server.MapPath("/app_data/logs/{0}/{1}.sqlite3".format(ph.dd.now().Year, ph.dd.now().ToString("yyyyMMddHHmmss")))\n        connstr = "Data Source={0};Version=3;Journal Mode=Off;Synchronous=Off;".format(path)\n        helper = DataHelper(connstr, "System.Data.SQLite")\n        fiInfo = FileInfo(path)\n        if not fiInfo.Exists:\n            if not fiInfo.Directory.Exists:\n                fiInfo.Directory.Create()\n            ddl = "CREATE TABLE log(id INTEGER NOT NULL, logger TEXT NOT NULL, logdate TEXT NOT NULL, thread INTEGER NOT NULL, loglevel TEXT NOT NULL, message TEXT, exception TEXT, userid TEXT, url TEXT, ip TEXT, browser TEXT, PRIMARY KEY(id ASC));"\n            helper.Exec(ddl)\n        dol = "insert into log(logger,logdate,thread,loglevel,message,exception,userid,url,ip,browser) values(@logger,@logdate,@thread,@loglevel,@message,@exception,@userid,@url,@ip,@browser)"\n        logs = dh.Objs("select * from dd.dd_log")\n        cmds = [ helper.Cmd(dol, log) for log in logs ]\n        affected = helper.Exec(cmds)\n        return jh.Affect(affected)\ndd_formbutton()	t	998,999	f	0		2015-07-15 11:36:42	2018-05-05 21:21:04.29137	\N	
dd_pip	DD-PIP	class dd_pip:\n    def getModel(self, prefix):\n        model = ph.dd.dynamic()\n        model.prefix = prefix\n        tables, left = ph.dd.dynamic(), len(prefix)\n        tables.dd_api = dh.Objs("select t1.* from dd.dd_api t1 where left(concat(oschm,'_',oname,'_',omethod),@len)=@prefix", left, prefix)\n        tables.dd_view = dh.Objs("select t1.* from dd.dd_view t1 where left(concat(oschm,'_',oname,'_',omethod),@len)=@prefix", left, prefix)\n        tables.dd_form = dh.Objs("select t1.* from dd.dd_form t1 where left(formname,@len)=@prefix", left, prefix)\n        tables.dd_element = dh.Objs("select t2.* from (select * from dd.dd_form where left(formname,@len)=@prefix) t1 inner join dd.dd_element t2 on t1.id=t2.formid", left, prefix)\n        tables.dd_formevent = dh.Objs("select t2.* from (select * from dd.dd_form where left(formname,@len)=@prefix) t1 inner join dd.dd_formevent t2 on t1.id=t2.formid", left, prefix)\n        tables.dd_formtrigger = dh.Objs("select t2.* from (select distinct relschm,relname from dd.dd_form where left(formname,@len)=@prefix) t1 inner join dd.dd_formtrigger t2 on t1.relschm=t2.relschm and t1.relname=t2.relname", left, prefix)\n        tables.dd_report = dh.Objs("select t1.* from dd.dd_report t1 where left(oname,@len)=@prefix", left, prefix)\n        tables.dd_fsm = dh.Objs("select t1.* from dd.dd_fsm t1 where left(id,@len)=@prefix", left, prefix)\n        tables.dd_code = dh.Objs("select t1.* from dd.dd_code t1 where left(oname,@len)=@prefix", left, prefix)\n        tables.dd_codedata = dh.Objs("select t2.* from (select * from dd.dd_code t1 where left(oname,@len)=@prefix) t1 inner join dd.dd_codedata t2 on t1.oname=t2.codename", left, prefix)\n        tables.dd_libpython = dh.Objs("select t1.* from dd.dd_libpython t1 where left(id,@len)=@prefix", left, prefix)\n        tables.dd_liblogin = dh.Objs("select t1.* from dd.dd_liblogin t1 where left(id,@len)=@prefix", left, prefix)\n        tables.dd_chart = dh.Objs("select t1.* from dd.dd_chart t1 where left(oname,@len)=@prefix", left, prefix)\n        tables.dd_libquery = dh.Objs("select t1.* from dd.dd_libquery t1 where left(queryname,@len)=@prefix", left, prefix)\n        tables.dd_formbutton = dh.Objs("select t2.* from (select id from dd.dd_form where left(formname,@len)=@prefix union all select id from dd.dd_libquery where left(queryname,@len)=@prefix) t1 inner join dd.dd_formbutton t2 on t1.id=t2.formid", left, prefix)\n        tables.dd_libsql = dh.Objs("select t1.* from dd.dd_libsql t1 where left(id,@len)=@prefix", left, prefix)\n        tables.dd_relmtm = dh.Objs("select t1.* from dd.dd_relmtm t1 where left(id,@len)=@prefix", left, prefix)\n        tables.dd_route = dh.Objs("select t1.* from dd.dd_route t1 where left(id,@len)=@prefix", left, prefix)        \n        model.db = dh.ProviderName\n        model.tables = tables\n        return jh.Encode(model)\n    def export(self, prefix, key="equiclouds"):\n        if not args.prefix: return "Missing prefix."\n        from System.Text.Encoding import UTF8\n        json = self.getModel(prefix)\n        content = ph.dd.zipaes(True, key).Encrypt(UTF8.GetBytes(json))\n        return controller.Action( { "filename": "{0}.pkg".format(prefix), "filecontent": content } )\n    def _cmp(self, remote, local):\n        insert, update, delete = ph.dd.dynamics(3)\n        keys = [ table.Key for table in remote ]   \n        for key in keys:\n            query = linq(remote[key]).FullJoin(linq(local[key]), lambda item: item.id, lambda item: item.id, lambda x,y: (key, x, y))\n            insert[key] = query.Where(lambda item: not (item[1] is None) and (item[2] is None) ).Select(lambda item: item[1]).ToArray()\n            update[key] = query.Where(lambda item: not (item[1] is None) and not (item[2] is None) and item[1].updatedtime != item[2].updatedtime ).Select(lambda item: item[1]).ToArray()\n            delete[key] = query.Where(lambda item: (item[1] is None) and not (item[2] is None) ).Select(lambda item: item[2]).ToArray()\n        model = ph.dd.dynamic()\n        model.insert, model.update, model.delete, model.tables = insert, update, delete, keys\n        return model\n    def _getRemote(self, url, file, key):        \n        from System.Text.Encoding import UTF8\n        bytes = ph.dd.resbd(url) if url else ph.dd.stream2bytes(file.InputStream)\n        return UTF8.GetString(ph.dd.zipaes(True, key).Decrypt(bytes))                \n    def _getCmds(self, cmpModel):\n        cmds, model = [], ph.dd.dynamic()\n        for table in cmpModel.tables:\n            inserts = cmpModel.insert[table] if args.insert else []\n            updates = cmpModel.update[table] if args.update else []\n            deletes = cmpModel.delete[table] if args.delete else []\n            model[table] = (inserts, updates, deletes)\n            cmds = cmds + list(db.Set(table).Cmds(inserts, updates, deletes))\n        return cmds, model\n    def install(self, key="equiclouds"):\n        url, file = args.url, Request.Files["file"]\n        if not args.url and not (file and file.ContentLength>0): return "Missing url or file."\n        remoteModel = jh.Decode(self._getRemote(url, file, key))\n        if not remoteModel.prefix: return "Missing prefix."\n        if args.verify_db and remoteModel.db != dh.ProviderName: return "Database type mismatch."\n        localModel = jh.Decode(self.getModel(remoteModel.prefix))\n        cmpModel = self._cmp(remoteModel.tables, localModel.tables)\n        if args.preview: return cmpModel\n        cmds, model = self._getCmds(cmpModel)\n        total = dh.Exec(cmds)\n        rslt = ["{0}\\tA{1}\\tU{2}\\tD{3}".format(table.Key.ljust(20), len(table.Value[0]), len(table.Value[1]), len(table.Value[2])) for table in model] + [ "total affected: {0}".format(total) ]\n        return "\\r\\n".join(rslt)\ndef main():    \n    return dd_pip()\nmain()	t	998,999	f	\N		2018-05-05 21:21:04.290373	2018-03-28 16:20:22	\N	
